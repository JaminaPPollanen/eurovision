{
  "expo": {
    "name": "eurovisionapp",
    "slug": "eurovisionapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#060a2f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#060a2f"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Eurovision app is created for everyone who is interested about eurovision song contest! You can follow time before semifinals, final, add favorites and look all the countries participate to contest!"
  }
}
